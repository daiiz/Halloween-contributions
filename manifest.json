{
  "name": "Halloween-contributions",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "GitHubのContributionsをかぼちゃ色に染めます。",
  "icons": { "128": "icon.png" },
  "content_scripts": [
      {
        "matches": ["https://github.com/*"],
        "css": ["main.css"],
        "run_at": "document_end"
      }
  ],
  "permissions": ["https://github.com/*"]
}
